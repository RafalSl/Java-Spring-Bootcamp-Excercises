package FirstSteps.Model;

public class Info {
	String name, lastname, position;
	int age;
	
	public Info() {
		name = "nieznane";
		lastname = "nieznane";
		position = "zwyk³y pracownik";
		age = 0;
	}
	
	public Info(String name, String lastname, String position, int age) {
		this.name = name;
		this.lastname = lastname;
		this.position = position;
		this.age = age;
	}
}
