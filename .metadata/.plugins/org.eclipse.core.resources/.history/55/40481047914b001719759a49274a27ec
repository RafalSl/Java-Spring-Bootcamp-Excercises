package FirstSteps.Model;

public class Info {
	private String name, lastname, position;
	private int gender;
	private int age;
	
	public Info() {
		name = "nieznane";
		lastname = "nieznane";
		position = "zwyk³y pracownik";
		age = 0;
		gender = 1;
	}
	
	public Info(String name, String lastname, String position, int age, int gender) {
		this.name = name;
		this.lastname = lastname;
		this.position = position;
		this.age = age;
		this.gender = gender;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLastname() {
		return lastname;
	}

	public void setLastname(String lastname) {
		this.lastname = lastname;
	}

	public String getPosition() {
		return position;
	}

	public void setPosition(String position) {
		this.position = position;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public int getGender() {
		return gender;
	}

	public void setGender(int gender) {
		this.gender = gender;
	}
	
	
}
